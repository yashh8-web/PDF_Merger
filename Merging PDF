      "source": [
        "from PyPDF2 import PdfMerger\n",
        "\n",
        "# Paths to your PDF files (replace with your file paths if needed)\n",
        "file1 = \"part1.pdf\"\n",
        "file2 = \"part2.pdf\"\n",
        "output_file = \"merged_result.pdf\"\n",
        "\n",
        "# Create a merger object\n",
        "merger = PdfMerger()\n",
        "\n",
        "# Append the PDFs one by one\n",
        "merger.append(file1)\n",
        "merger.append(file2)\n",
        "\n",
        "# Write the final output\n",
        "merger.write(output_file)\n",
        "merger.close()\n",
        "\n",
        "print(f\"✅ Merged PDF saved as: {output_file}\")"
      ],
